{
    "manifest_version": 3,
    "name": "码云平台 Issue/PR 自动填充默认值扩展插件",
    "version": "2.0.0",
    "description": "码云平台 Issue/Pull Request 自动填充默认值扩展插件",
    "author": "argo zhang",
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [{
        "matches": [
            "https://gitee.com/Longbow/GiteeIssueExtensions/issues/*",
            "https://gitee.com/Longbow/GiteeIssueExtensions/pulls/*",
            "https://gitee.com/LongbowEnterprise/BootstrapBlazor/issues/*",
            "https://gitee.com/LongbowEnterprise/BootstrapBlazor/pulls/*",
            "https://gitee.com/LongbowEnterprise/BootstrapBlazor/pull/new/*",
            "https://gitee.com/LongbowEnterprise/BootstrapAdmin/issues/*",
            "https://gitee.com/LongbowEnterprise/BootstrapAdmin/pulls/*",
            "https://gitee.com/LongbowEnterprise/BootstrapAdmin/pull/new/*"
        ],
        "js": ["content-script.js"]
    }]
}